.page {
    background: var(--background-color);
}

.container {
    background: var(--background-color);
}

.nav {
    background: #fff;
}

.nav-title {
    color: #333333 !important;
    font-weight: 500 !important;
    font-size: 38rpx !important;
}

.leftIcon {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 72rpx;
    color: #333333;
    transform: translateX(-25%);
}

.leftIcon .iconfont {
    font-size: 36rpx;
}

.content-box {
    padding-bottom: env(safe-area-inset-bottom);
    width: 100%;
}

.favorites-tabs {
    position: fixed;
    left: 0;
    z-index: 21;
    width: 100%;
    height: 100rpx;
    border-radius: 0 0 30rpx 30rpx !important;
    background: #fff;
}

.van-tabs--line .van-tabs__wrap,
.van-tabs__nav {
    height: 94rpx !important;
}

.van-tabs__line {
    width: 48rpx !important;
    height: 8rpx !important;
    border-radius: 0 !important;
    background: var(--primary-gradual-color);
}

.van-tab {
    color: var(--primary-color-text) !important;
    font-size: 28rpx !important;
    line-height: 100rpx !important;
}

.van-tab--active {
    font-weight: 600 !important;
}

.van-tabs--line .van-tabs__wrap {
    padding-bottom: 14rpx !important;
    border-radius: 0 0 30rpx 30rpx !important;
}

.van-tabs__scroll--line {
    height: calc(100% + 22rpx) !important;
}

.favorites-list {
    padding-top: 100rpx;
    /* padding-bottom: calc(80rpx + env(safe-area-inset-bottom)); */
}
.favorites-list-padding {
    padding-bottom: calc(80rpx + env(safe-area-inset-bottom));
}

.list-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 0 0 30rpx;
    height: 100rpx;
    color: #564b4e;
    font-weight: 400;
    font-size: 28rpx;
    font-family: PingFangSC-Regular, PingFang SC;
    line-height: 40rpx;
}

.favorites-list-manage {
    padding: 16rpx 30rpx;
}

.list-content {
    display: flex;
    overflow: hidden;
    align-items: center;
    justify-content: flex-start;
    box-sizing: border-box;
    margin-bottom: 24rpx;
    padding-right: 30rpx;
    width: 100%;
    height: 280rpx;
    border-radius: 30rpx;
    background: #fff;
}

.checkbox {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    padding-left: 30rpx;
    width: 90rpx;
    height: 200rpx;
}

/* 选中框自定义 */
.van-checkbox__label {
    --checkbox-label-margin: 16rpx !important;
    --checkbox-label-color: var(--primary-color-text) !important;
}

.van-checkbox__icon--checked {
    background-color: #fff !important;
    color: var(--primary-color) !important;

    --checkbox-checked-icon-color: var(--primary-color) !important;
}

.van-checkbox__icon {
    --checkbox-border-color: var(--primary-color) !important;
}

.custom-check-box-icon {
    display: -webkit-flex;
    display: flex;
    overflow: hidden;
    -webkit-align-items: center;
    align-items: center;
    width: 40rpx;
    height: 40rpx;
    color: var(--primary-color);
    font-size: 38rpx;

    -webkit-user-select: none;
    user-select: none;
}

.goods-info {
    display: flex;
    align-items: center;
    box-sizing: border-box;
    width: 100%;
}

.img-box {
    padding-right: 20rpx;
}

.text-box {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    width: 100%;
    height: 200rpx;
}

.goods-title {
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    color: var(--primary-color-text);
    text-overflow: ellipsis;
    word-break: break-all;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;

    -webkit-line-clamp: 2;
}

.text-box-bottom-wrap {
    display: flex;
    align-items: center;
    /* justify-content: space-between; */
}

.goods-price {
}
.price-now-unit {
    bottom: 0;
    display: inline-block;
    color: var(--features-color-text);
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
}

.price-now {
    bottom: 0;
    display: inline-block;
    color: var(--features-color-text);
    font-weight: 500;
    font-size: 36rpx;
    line-height: 50rpx;
}
.color-invalid {
    color: var(--tip-color-text);
}
.price-cut {
    margin-left: 24rpx;
    padding: 0 10rpx;
    height: 38rpx;
    border-radius: 20rpx;
    background: var(--secondary-color);
    color: var(--features-color-text);
    font-size: 24rpx;
    line-height: 38rpx;
}
.invalid {
    margin-left: auto;
    padding: 0 22rpx;
    height: 50rpx;
    border-radius: 26rpx;
    background: var(--tip-color-text-88);
    color: #ffffff;
    font-size: 24rpx;
    line-height: 50rpx;
}
.invalid-goods {
    color: var(--tip-color-text) !important;
}

.edit_model {
    color: var(--features-color-text) !important;
}

/* 批量删除 */
.del-submit-bar {
    position: fixed;
    bottom: 0;
    z-index: 11;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding-bottom: env(safe-area-inset-bottom);
    /* bottom: var(_submit_bar_bottom_); */
    width: 100%;
    height: 100rpx;
    background-color: #ffffff;
    box-shadow: 0px -1px 0px 0px rgba(209, 201, 203, 0.2);
}

.del-submit-bar-button {
    margin-right: 30rpx;
    padding: 16rpx 74rpx;
    border: 2rpx solid var(--primary-color);
    border-radius: 34rpx;
    color: var(--features-color-text);
    font-weight: 400;
    font-size: 26rpx;
    line-height: 36rpx;
}

.del-submit-bar-button.van-button--round.van-button--normal {
    width: 200rpx !important;
    height: 68rpx !important;
}

.del-submit-bar-button-disable {
    opacity: 0.5;
}

/* 删除确认框 */
.dialog-index--van-dialog__cancel {
    width: 100% !important;
    color: var(--primary-color-text) important;
    font-weight: 400;
    font-size: 32rpx;
}

.dialog-index--van-dialog__confirm {
    width: 100% !important;
    color: var(--primary-color-text) important;
    font-weight: 400;
    font-size: 32rpx;
}

.van-dialog__message {
    color: var(--primary-color-text);
    font-weight: 500;

    --dialog-message-font-size: 32rpx;
    --dialog-message-line-height: 44rpx;
}

.van-dialog__button {
    display: flex;
    justify-content: center;
}

.botton-hover {
    opacity: 0.5;
}
.bottom-loading {
    margin: 25rpx 0;
    width: 100vw;
}
.no-more {
    padding: 25rpx 0;
    width: 100%;
    color: var(--secondary-color-text);
    text-align: center;
    font-size: 22rpx;
}
/* 看看 */
.kk-list {
    display: flex;
    align-items: flex-start;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-evenly;
    margin-bottom: 20rpx;
    padding-top: 110rpx;
    width: 100%;
}
.kk-list .item-wrap {
    width: 340rpx;
}
.kk-list .item {
    position: relative;
    align-self: end;
    /* flex: auto; */
    margin-top: 20rpx;
    padding-bottom: 24rpx;
    width: 340rpx;
    border-radius: 10rpx;
    background-color: #fff;
}
.kk-list .item .timg {
    width: 340rpx;
    height: 340rpx;
    border-radius: 10rpx 10rpx 0px 0px;
}
.kk-list .item .info {
    box-sizing: border-box;
    padding: 0 20rpx;
}
.kk-list .item .info .title {
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    margin-top: 16rpx;
    word-break: break-all;
    font-weight: 500;
    font-size: 30rpx;

    -webkit-line-clamp: 2;
}
.kk-list .item .info .author {
    display: flex;
    align-items: center;
    margin-top: 16rpx;
}
.kk-list .item .info .author .avator {
    flex-shrink: 0;
    width: 36rpx;
    height: 36rpx;
    border-radius: 18rpx;
}
.kk-list .item .info .author .name {
    margin-left: 12rpx;
    color: var(--tip-color-text-a2);
    font-size: 26rpx;
}
.kk-list .item .num {
    position: absolute;
    top: 292rpx;
    right: 8rpx;
    display: flex;
    align-items: center;
    flex-grow: 0;
    padding: 0 12rpx 0 16rpx;
    height: 36rpx;
    border-radius: 18rpx;
    background: rgba(0, 0, 0, 0.5);
    color: #fff;
    font-size: 22rpx;
}
.kk-list .item .num .iconfont {
    margin-left: 8rpx;
    color: #fff;
    font-size: 28rpx;
}
.no-more {
    padding: 60rpx 0;
    width: 100%;
    color: var(--secondary-color-text);
    text-align: center;
    font-size: 22rpx;
}
