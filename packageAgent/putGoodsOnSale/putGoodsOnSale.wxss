.container {
    background: #fff;
}
.nav {
    background: #fff;
}
.background-F5F5F5 {
    background: var(--background-color);
}

.title {
    color: #333333 !important;
    font-weight: 500 !important;
    font-size: 38rpx !important;
}

.leftIcon {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 72rpx;
    color: #333333;
    transform: translateX(-25%);
}
.leftIcon .iconfont {
    font-size: 36rpx;
}
.content-box {
    width: 100%;
    /*padding-bottom: 100rpx;*/
}
/*搜索框*/
.search-box {
    display: flex;
    align-items: center;
    justify-content: space-between;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 8rpx 30rpx;
    width: 750rpx;
    height: 88rpx;
    background: #fff;
}
.search-box .search-bar {
    position: relative;
    width: 690rpx;
    height: 72rpx;
}
.search-box .search-bar .search-input-box {
    width: 100%;
    height: 100%;
}
.search-box .search-bar .search-input {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 72rpx;
    width: 100%;
    height: 100%;
    border-radius: 36rpx;
    background: var(--search-input);
    color: var(--primary-color-text);
    font-size: 26rpx;
}
.search-box .icon-glass {
    position: absolute;
    top: 16rpx;
    left: 28rpx;
    color: var(--secondary-color-text);
    font-size: 36rpx;
}
.search-box .close-icon {
    position: absolute;
    top: 0;
    right: 12rpx;
    padding: 16rpx;
}
.search-input-placeholder {
    color: var(--secondary-color-text);
    font-weight: 400;
    font-size: 26rpx;
}
.search-box .search-bar-activity {
    width: 610rpx;
}
.search-cancel {
    padding-left: 20rpx;
    width: 60rpx;
    height: 100%;
    color: var(--primary-color-text);
    font-weight: 400;
    font-size: 30rpx;
    line-height: 76rpx;
}

.clearfix:after {
    display: block;
    visibility: hidden;
    clear: both;
    height: 0;
    content: ' ';
    font-size: 0;
}

.icon-cross {
    position: absolute;
    top: 50%;
    right: 13rpx;
    padding: 0 5rpx;
    width: 36rpx;
    height: 100%;
    transform: translateY(-50%);
}

/**/
.contentFixed {
    position: fixed;
    top: 66rpx;
    left: 0;
    z-index: 1;
}

/*悬浮容器--条件筛选导航栏 + 批量上架*/
.sticky-placeholder-box {
    width: 100%;
    height: 268rpx;
}
.h88 {
    height: 88rpx;
}
.sticky-box {
    position: fixed;
    z-index: 10;
    width: 100%;
    background: #ffffff;
}
.filter-nav {
    display: flex;
    width: 100%;
    height: 108rpx;
    background: #fff;
}
.filter-nav-item {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 33.3%;
    color: var(--primary-color-text);
    font-weight: 400;
    font-size: 28rpx;
}
.filter-nav-selected {
    color: var(--primary-color-text);
    font-weight: 600;
}
.filter-selected {
    color: var(--features-color-text);
    font-weight: 500;
}
.no-selected {
    color: var(--primary-color-text);
    font-weight: 400;
}

/*筛选*/
.filter-nav-text {
    position: relative;
    height: 40rpx;
    line-height: 40rpx;
}

.filter-nav-icon {
    margin-left: 8rpx;
    /*height: 74rpx;*/
    font-size: 28rpx;
}

.icon-sort {
    display: inline-block;
    margin-left: 8rpx;
    width: 16rpx;
    height: 24rpx;
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
    vertical-align: bottom;
}

.sort-arrow {
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-left: 10rpx;
}
.sort-arrow .iconfont {
    color: var(--primary-color-text);
    font-size: 12rpx;
}

.sort-is-selected {
    position: absolute;
    top: 40rpx;
    left: 14rpx;
    width: 30rpx;
    height: 16rpx;
}

.icon-descending {
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
}

.icon-ascending {
    background: url('data:image/png;base64,');
    background-size: 100% 100%;
}

.sort-reverse {
    transform: rotate(180deg);
}
/*批量上架*/
.batch-shelves {
    display: flex;
    align-items: center;
    justify-content: space-between;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 30rpx;
    width: 100%;
    height: 72rpx;
    background: var(--secondary-background);
    color: var(--features-color-text);
    font-weight: 400;
    font-size: 26rpx;
    line-height: 36px;
}
.btn-batch-shelves {
    width: 160rpx !important;
    color: #ffffff;
    font-weight: 400;
    font-size: 26rpx !important;
}
.batch-shelves .van-button--normal {
    padding: 0 !important;
}
.batch-shelves .van-button--round.van-button--normal {
    height: 56rpx !important;
    line-height: 56rpx !important;
}
.batch-shelves van-button {
    display: flex;
    align-items: center;
    height: 56rpx !important;
}

/*商品列表*/
.goods-list {
    margin: 0 auto;
    padding: 24rpx 0 40rpx;
    width: 100%;
}
.goods-list-content {
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    width: 100%;
}

/*空列表*/
.empty-list {
    display: flex;
    align-items: center;
    flex-direction: column;
    padding-top: 160rpx;
    width: 100%;
}
.empty-img {
    display: inline-block;
    width: 202rpx;
    height: 224rpx;
    vertical-align: bottom;
}
.empty-text {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin-top: 40rpx;
    padding-right: 36rpx;
    width: 100%;
    color: var(--primary-color);
    text-align: center;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 30rpx;
}
.van-loading {
    padding: 20rpx 0 10rpx;
    width: 100%;
}
.no-more {
    padding: 20rpx 0 10rpx;
    width: 100%;
    color: var(--secondary-color-text);
    text-align: center;
    font-size: 22rpx;
}

/* agentGoods组件 title插槽 */
.slot-title {
    padding-bottom: 30rpx;
    height: 44rpx;
    color: #333333;
    font-weight: 500;
    font-size: 32rpx;
    line-height: 44rpx;
}
/* agentGoods组件 checkBox插槽 */
.check-box {
    display: flex;
    align-items: center;
    padding: 0 16rpx 0 20rpx;
    height: 136rpx;
}
.van-checkbox__icon--checked {
    background-color: #fff !important;
    color: var(--primary-color) !important;

    --checkbox-checked-icon-color: var(--primary-color) !important;
}

.van-checkbox__icon {
    --checkbox-border-color: var(--primary-color) !important;
}

.custom-check-box-icon {
    display: -webkit-flex;
    display: flex;
    overflow: hidden;
    -webkit-align-items: center;
    align-items: center;
    width: 28rpx;
    height: 28rpx;

    -webkit-user-select: none;
    user-select: none;
}
/* agentGoods组件 bottom插槽 */
.goods-btn-box {
    display: flex;
    justify-content: flex-end;
    width: 100%;
    height: 56rpx;
    /*padding-bottom: 30rpx;*/
}
.goods-btn-box .operation-btn {
    height: 56rpx !important;
    color: #ffffff;
    font-weight: 400;
    font-size: 26rpx !important;
}
.w160 {
    width: 160rpx !important;
}
.w128 {
    width: 128rpx !important;
}
.goods-btn-box button {
    margin-left: 24rpx;
    padding: 0 !important;
}
.goods-btn-box .van-button--round.van-button--normal {
    height: 56rpx !important;
    line-height: 56rpx !important;
}
.goods-btn-box van-button {
    display: flex;
    align-items: center;
    height: 56rpx !important;
}

.hide {
    display: none;
}

/*批量上架 底部容器*/
.bottom-box {
    position: fixed;
    bottom: 0;
    left: 0;
    padding: 0 0 env(safe-area-inset-bottom) 0;
    width: 100%;
    height: 100rpx;
    background: #fff;
    box-shadow: 0 -1px 0 0 rgba(0, 0, 0, 0.08);
}
.bottom-box .bottom-content {
    display: flex;
    align-items: center;
    justify-content: space-between;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding: 0 30rpx;
    width: 100%;
    height: 100rpx;
}
.select-all {
    display: flex;
    align-items: center;
    padding-left: 54rpx;
    width: 200rpx;
    height: 100%;
}
.selected-all-text {
    margin-left: 16rpx;
}

.btn-sure-batch-shelves {
    width: 200rpx !important;
    color: #ffffff;
    font-weight: 400;
    font-size: 26rpx !important;
}
.bottom-box .van-button--round.van-button--normal {
    height: 68rpx !important;
    line-height: 68rpx !important;
}
.bottom-box van-button {
    display: flex;
    align-items: center;
    height: 68rpx !important;
}
.all-check-box-icon {
    display: -webkit-flex;
    display: flex;
    overflow: hidden;
    -webkit-align-items: center;
    align-items: center;
    width: 40rpx;
    height: 40rpx;

    -webkit-user-select: none;
    user-select: none;
}
.bottom-box-show {
    padding-bottom: 140rpx;
}
/* 批量上架 调整价格弹框 */
.batch-shelves-popup {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    padding-top: 30rpx;
    width: 690rpx;
    height: 476rpx;
    border-radius: 30rpx;
    background: #ffffff;
}

.popup-title {
    width: 100%;
    height: 44rpx;
    color: var(--primary-color-text);
    text-align: center;
    font-weight: 500;
    font-size: 32rpx;
    line-height: 44rpx;
}

.shelves-bottom-tip {
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
    margin: 40rpx auto 0;
    height: 34rpx;
    color: var(--tip-color-text);
    text-align: center;
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
}
.shelves-bottom-button-box {
    display: flex;
    justify-content: space-between;
    margin: 60rpx auto 0;
    width: 460rpx;
    font-weight: 400 !important;
    font-size: 28rpx !important;
    line-height: 40rpx !important;
}

.shelves-btn-cancel {
    width: 200rpx !important;
    height: 80px !important;
    border: 0px solid #ebedf0 !important;
    border-radius: 40rpx !important;
    background: linear-gradient(90deg, #d6dee9 0%, #b3bac1 100%) !important;
    box-shadow: 0 6rpx 12rpx 0 rgba(185, 185, 185, 0.5);
    color: #ffffff !important;
}

.shelves-btn-confirm {
    width: 200rpx !important;
    height: 80rpx !important;
    color: #ffffff !important;
}

.batch-price-stepper {
    margin: 60rpx auto 0;
    width: 506rpx;
}
