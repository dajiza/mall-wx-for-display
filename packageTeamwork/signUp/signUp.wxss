/* pages/orderCheck/orderCheck.wxss */
.container {
    padding-bottom: calc(140rpx + env(safe-area-inset-bottom));
    background-color: var(--background-color);
}
.wrap {
    width: 100%;
    border-radius: 30rpx;
    background-color: #fff;
}
.nav {
    background: #fff !important;
}
.nav-title {
    color: #333 !important;
    font-weight: 500;
    font-size: 38rpx !important;
}
.leftIcon .iconfont {
    font-size: 36rpx;
}
.van-nav {
    width: 100%;
    background: #fff !important;
}

.column {
    box-sizing: border-box;
    margin-top: 30rpx;
    padding: 0 30rpx;
    width: 100%;
    background: #ffffff;
}
.item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 100%;
    height: 130rpx;
    font-size: 30rpx;
    line-height: 130rpx;
}

.container .btn-wrap {
    position: fixed;
    right: 0;
    bottom: calc(40rpx + env(safe-area-inset-bottom));
    left: 0;
    display: flex;
    justify-content: space-between;
    margin: 0 auto;
    width: 690rpx;
}
.container .big-btn {
    width: 690rpx !important;
}

