.tips {
    margin: 30rpx 30rpx 0rpx 0;
    font-size: 24rpx;
    line-height: 34rpx;
}
.check {
    margin-left: 40rpx;
    width: 76rpx;
    height: 48rpx;
    border-radius: 8rpx;
    background: #ffffff;
    box-shadow: 0px 0px 8rpx 0px rgba(0, 0, 0, 0.15);
    color: var(--primary-color);
    text-align: center;
    font-size: 26rpx;
    line-height: 48rpx;
}
.input-invite {
    margin-left: auto;
}
.check-status {
    margin-left: 20rpx;
    width: 28rpx;
    height: 28rpx;
}
.check-name {
    display: flex;
    align-items: center;
}

.pay-tips {
    margin: 20rpx 0 0 66rpx;
    color: var(--tip-color-text);
    font-size: 24rpx;
    line-height: 38rpx;
}
