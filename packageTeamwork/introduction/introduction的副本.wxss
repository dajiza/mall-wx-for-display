/* pages/orderCheck/orderCheck.wxss */
.container {
    padding-bottom: calc(140rpx + env(safe-area-inset-bottom));
    background-color: #fff;
}
.wrap {
    width: 100%;
    border-radius: 30rpx;
    background-color: #fff;
}
.nav {
    background: #fff !important;
}
.nav-title {
    color: #333 !important;
    font-weight: 500;
    font-size: 38rpx !important;
}
.leftIcon .iconfont {
    font-size: 36rpx;
}
.van-nav {
    width: 100%;
    background: #fff !important;
}
.blank {
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-top: 200rpx;
    width: 100%;
}
.blank .add {
    width: 170rpx;
    height: 170rpx;
    border-radius: 50%;
    background: var(--primary-gradual-color);
    text-align: center;
    line-height: 170rpx;
}
.blank .add .icon-jiahao {
    color: #fff;
    font-size: 164rpx;
}
.blank .text {
    margin-top: 60rpx;
}
/* 内容编辑 */
.substance {
    width: 100%;
}
/* 弹框 */
.custom-share {
    right: 0;
    left: 0;
    margin: auto;
    height: 288rpx;
    border-radius: 30rpx 30rpx 0 0;
    background: #ffffff;
}
.custom-share .share-wrap {
    width: 100%;
    height: 100%;
}

.custom-share .share-wrap .item-list {
    display: flex;
    justify-content: space-around;
    padding-top: 60rpx;
}
.custom-share .share-wrap .item-list .item {
    display: flex;
    align-items: center;
    flex-direction: column;
}
.custom-share .share-wrap .share-img {
    width: 100rpx;
    height: 100rpx;
}
.custom-share .share-wrap .text {
    padding-top: 12rpx;
    color: #333333;
    font-size: 24rpx;
}
