/* pages/orderCheck/orderCheck.wxss */
.container {
    padding-bottom: calc(140rpx + env(safe-area-inset-bottom));
    background-color: #fff;
}
.wrap {
    width: 100%;
    border-radius: 30rpx;
    background-color: #fff;
}
.nav {
    background: #fff !important;
}
.nav-title {
    color: #333 !important;
    font-weight: 500;
    font-size: 38rpx !important;
}
.leftIcon .iconfont {
    font-size: 36rpx;
}
.van-nav {
    width: 100%;
    background: #fff !important;
}
/* 内容 */
.blank {
    display: flex;
    align-items: center;
    flex-direction: column;
    margin-top: 260rpx;
    width: 100%;
}
.blank .icon {
    margin-left: 30rpx;
    width: 183rpx;
    height: 160rpx;
}

.blank .text {
    margin-top: 40rpx;
    font-size: 32rpx;
}

.container .btn-wrap {
    margin: 80rpx auto 0;
}
.container .big-btn {
    width: 320rpx !important;
}

.btn-wrap-2 {
    display: flex;
    justify-content: center;
    padding-top: 60rpx;
    padding-bottom: 30rpx;
}

.middle-btn {
    width: 320rpx !important;
}
