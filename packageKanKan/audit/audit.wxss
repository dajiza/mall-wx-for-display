page {
  overflow: hidde;
  box-sizing: border-box;
  height: 100vh;
  background-color: #ffffff;
}

.title {
  color: #333333 !important;
  font-weight: 500 !important;
  font-size: 38rpx !important;
}

.leftIcon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 72rpx;
  color: #333333;
  transform: translateX(-25%);
}

.leftIcon .iconfont {
  font-size: 36rpx;
}

.verify-content {
  width: 100%;
  margin-top: 144rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.verify-img {
  width: 382rpx;
  height: 310rpx;
}

.verify-status {
  font-size: 32rpx;
  font-weight: 400;
  color: var(--primary-color-text);
  line-height: 44rpx;
  margin-top: 60rpx;
}

.botton-hover {
  opacity: 0.8;
}

.bottom-button {
  display: flex;
  align-items: center;
  justify-content: center;
  margin: 0 auto;
  width: 280rpx;
  height: 88rpx;
  border-radius: 44rpx;
  background: var(--primary-color-btn);
  color: #ffffff;
  line-height: 88rpx;
  margin-top: 120rpx;
}

.bottom-button-container-label {
  height: 40rpx;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
}

.btn-second {
  font-size: 26rpx;
  font-weight: 400;
  color: #A2A2A2;
  line-height: 36rpx;
  margin-top: 40rpx;
}