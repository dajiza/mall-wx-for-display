page {
  /*overflow: hidden;*/
  box-sizing: border-box;
  height: 100vh;
  background-color: #ffffff;
}
.container {
  position: relative;
  padding-bottom: calc(150rpx + env(safe-area-inset-bottom));
  background: var(--background-color);
}

.nav{
  background-color: transparent !important;
}

/*.title {*/
/*  color: #ffffff !important;*/
/*  font-weight: 500 !important;*/
/*  font-size: 38rpx !important;*/
/*}*/

.leftIcon {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 72rpx;
  color: #ffffff !important;
  transform: translateX(-25%);
}

.leftIcon .iconfont {
  font-size: 36rpx;
}

.swiper-box {
  width: 100%;
  height: 100%;
}
.swipercontent {
  position: relative;
  height: 100%;
}

.dots{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 40rpx;
  display: flex;
  justify-content: center;
}

.dot{
  margin: 0 8rpx;
  width: 20rpx;
  height: 8rpx;
  background: #fff;
  border-radius: 6rpx;
  transition: all .6s;
}
.dot.active{
  width: 28rpx;
  background: var(--primary-color);
}

.swiper-box .swiper-item {
  width: 100%;
  height: 100%;
}
.swiper-box .swiper-item .item-img {
  display: block;
  width: 100%;
  height: 100%;
  vertical-align: bottom;
}
/*轮播下标*/
.swiper-box .wx-swiper-dots.wx-swiper-dots-horizontal {
  margin-bottom: -10rpx;
}
.swiper-box .wx-swiper-dot {
  display: inline-flex;
  justify-content: space-between;
  margin-left: 4rpx;
  width: 20rpx;
  height: 4rpx;
}
.swiper-box .wx-swiper-dot::before {
  flex-grow: 1;
  border-radius: 3rpx;
  background: #ececec;
  content: '';
}
.swiper-box .wx-swiper-dot-active {
  width: 28rpx;
}
.swiper-box .wx-swiper-dot-active::before {
  background: var(--primary-color);
}
.swiper-box .banner-nav {
  width: 100%;
  height: 100%;
}
.title-author{
  width: 650rpx;
  /*height: 106px;*/
  background: #fff;
  border-radius: 20rpx;
  padding: 30rpx;
  margin: 24rpx auto 0;
}
.title-author .title-text{
  width: 100%;
  font-size: 36rpx;
  font-weight: 500;
  color: var(--primary-color-text);
  line-height: 50rpx;
}
.title-author .line{
  width: 650rpx;
  height: 2rpx;
  background: #fff;
  box-shadow: 0px 1rpx 0px 0px #EEEEEE;
  margin: 28rpx 0 30rpx;
}
.title-author .author-box {
  width: 100%;
  height: 40rpx;
  display: flex;
  align-items: center;
  font-weight: 400;
  color: var(--tip-color-text-a2);
}
.title-author .header-img{
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
}
.title-author .author-name{
  margin-left: 12rpx;
  font-size: 28rpx;
  font-weight: 400;
  line-height: 40rpx;
}
.collect-count{
  margin-left: auto;
  height: 34rpx;
  font-size: 24rpx;
  line-height: 34rpx;
}
.material-list{
  width: 100%;
  /*margin-top: 40rpx;*/
}
.material-scroll-view{
  /*padding: 0 20rpx;*/
  white-space: nowrap;
  height: 386rpx;
}
.placeholder-box{
  width: 20rpx;
  height: 100rpx;
  display: inline-block;
}
.material-item{
  width: 216rpx;
  height: 386rpx;
  background: #FFFFFF;
  border-radius: 10rpx;
  display: inline-block;
  margin-right: 12rpx;
  vertical-align: middle;
}
.material-item .material-img{
  width: 216rpx;
  height: 216rpx;
  border-radius: 10rpx 10rpx 0 0;
  /*background: #3333;*/
}
.material-item .title-price{
  box-sizing: border-box;
  height: 160rpx;
  color: var(--primary-color-text);
  font-size: 26rpx;
  font-weight: 400;
  line-height: 36rpx;
  padding: 12rpx 12rpx 0;
}
.material-item .material-title{
  height: 72rpx;
}
.material-item .material-price{
  height: 76rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: var(--primary-color);
  line-height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.material-item .price{

}
.material-item .car-icon-wrap{
  width: 52rpx;
  height: 52rpx;
  background: var(--primary-gradual-color);
  border-radius: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.material-item .car-icon-wrap .car-icon{
  width: 36rpx;
  height: 36rpx;
}
.text-overflow-2 {
  display: -webkit-box;
  overflow: hidden;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  white-space: normal !important;
  word-wrap: break-word;

  -webkit-line-clamp: 2;
}
.label-text{
  width: auto;
  height: 44rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: var(--primary-color-text);
  line-height: 44rpx;
  margin: 40rpx 30rpx 30rpx;
}
.course-list{
  width: 710rpx;
  padding: 36rpx 20rpx 48rpx;
  box-sizing: border-box;
  background: #ffffff;
  border-radius: 20rpx;
  margin: 0 auto;
}
.course-list .course-item{
  margin-top: 60rpx;
}
.course-list .course-item:first-child{
  margin-top: 0;
}
.course-item .course-title-desc{
  display: flex;
}
.course-item .course-title-desc .course-title{
  height: 40rpx;
  background: #FB6090;
  border-radius: 6rpx;
  display: inline-block;
  padding: 0 6rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 20px;
  min-width: 80rpx;
  text-align: center;
}
.course-count{
  font-size: 20rpx;
  color: rgba(255,255,255,.5);
}
.course-item .course-title-desc .course-desc{
  font-size: 28rpx;
  font-weight: 400;
  color: var(--primary-color-text);
  line-height: 44rpx;
  margin-left: 20rpx;
}
.course-item .course-img-box{
  width: 100%;
  margin-top: 30rpx;
}
.course-item .course-img{
  width: 100%;
  height: 400rpx;
}
.no-more{
  width: 100%;
  height: 34rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: var(--primary-color-text);
  line-height: 34rpx;
  box-shadow: 0px -1rpx 0px 0px #EEEEEE;
  padding-top: 30rpx;
  text-align: center;
  margin-top: 30rpx;
}
.bottom-bar{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  background: #FFFFFF;
  box-shadow: 0px -2rpx 0px 0px rgba(0, 0, 0, 0.08);
  display: flex;
  align-items: center;
  justify-content: center;
  padding-bottom: env(safe-area-inset-bottom);
}
.collect-box,.share-box{
  width: 50%;
  height: 100%;
  box-sizing: border-box;
  color: var(--primary-color-text);
  padding: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
}
.collect-box{
  padding-left: 70rpx;
}
.share-box{
  padding-right: 70rpx;
}
.w100{
  width: 100%;
  padding: 0;
}
.share-button{
  position: absolute;
  right: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: transparent !important;
}
.bar-label-text{
  font-size: 20rpx;
  font-weight: 400;
  line-height: 28rpx;
}
.icon-init{
  width: 48rpx;
  height: 48rpx;
}
.bottom-bar .iconfont{
  font-size: 48rpx;
  color: #564B4E;
}
.collect-box .icon-favorites-fill {
  color: var(--primary-color);
}