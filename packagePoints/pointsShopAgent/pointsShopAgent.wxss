page {
    background: var(--background-color);
}

.container {
    background: var(--background-color);
}

.nav {
    background: #fff;
}

.title {
    color: #333333 !important;
    font-weight: 500 !important;
    font-size: 38rpx !important;
}

.leftIcon {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 72rpx;
    color: #333333;
    transform: translateX(-25%);
}

.leftIcon .iconfont {
    font-size: 36rpx;
}

.header-fixed {
    position: fixed;
    left: 0;
    z-index: 2;
    width: 100%;
}

.van-tabs--line .van-tabs__wrap,
.van-tabs__nav {
    height: 86rpx !important;
}

.van-tabs__line {
    width: 52rpx !important;
    height: 8rpx !important;
    border-radius: 0 !important;
    background: var(--primary-gradual-color);
}

.tabs-rwap {
}

.van-tab {
    padding-top: 30rpx !important;
    color: var(--primary-color-text) !important;
    font-size: 28rpx !important;
    line-height: 40rpx !important;
}

.van-tab--active {
    font-weight: 600 !important;
}

.van-tabs--line .van-tabs__wrap {
    padding-bottom: 14rpx !important;
}

.van-tabs__scroll--line {
    height: calc(100% + 14rpx) !important;
}

.list-tab-wrap {
    display: flex;
    box-sizing: border-box;
    padding: 30rpx 40rpx;
    height: 128rpx;
    background: var(--background-color);
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.list-tab-active {
    display: flex;
    align-items: center;
    padding: 0 28rpx;
    height: 68rpx;
    border-radius: 34rpx;
    background: #ffdedf;
    color: var(--primary-color);
}

.list-tab-normal {
    display: flex;
    align-items: center;
    padding: 0 28rpx;
    height: 68rpx;
    border-radius: 34rpx;
    background: #efefef;
    color: var(--primary-color-text);
}

.content-wrap {
    position: relative;
    width: 100%;
}

.tab-goods-page {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
}

.tab-order-page {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
}

.tab-drop-down {
    margin-left: 12rpx;
    font-size: 28rpx;
}

.list-tab-page-hide {
    display: none;
}

.bottom-button-container {
    position: fixed;
    bottom: 0;
    z-index: 100;
    display: flex;
    flex-direction: column;
    width: 100%;
    background: #ffffff;
}

.botton-hover {
    opacity: 0.8;
}

.bottom-button {
    display: flex;
    align-items: center;
    justify-content: center;
    margin: 0 auto;
    width: 690rpx;
    height: 88rpx;
    border-radius: 44rpx;
    background: var(--primary-color-btn);
    color: #ffffff;
    line-height: 88rpx;
}

.bottom-button-container-label {
    height: 40rpx;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.drop-down-menu {
    padding: 10rpx;

    --popup-background-color: #00000000;
}

.goods-drop-down {
    width: 256rpx;
    border-radius: 10rpx;
    background: #ffffff;
    box-shadow: 0px 6rpx 12rpx 0px rgba(0, 0, 0, 0.1);
    color: var(--primary-color-text);
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.coupon-drop-down {
    width: 280rpx;
    border-radius: 10rpx;
    background: #ffffff;
    box-shadow: 0px 6rpx 12rpx 0px rgba(0, 0, 0, 0.1);
    color: var(--primary-color-text);
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.div-line {
    margin: 0 auto;
    width: 216rpx;
    height: 1rpx;
    background: #efefef;
}

.drop-down-item {
    box-sizing: border-box;
    padding: 20rpx 0;
    text-align: center;
}

.empty-list-box {
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    color: var(--scan-btn-background);
}

.empty-list-tip {
    color: var(--scan-btn-background);
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.empty-image-wrap {
    margin: 0 auto;
    margin-top: 170rpx;
    width: 234rpx;
    height: 254rpx;
}

.empty-image {
    margin-left: 32rpx;
    width: 202rpx;
    height: 224rpx;
}

.list-wrap {
    display: flex;
    padding-right: 30rpx;
    padding-left: 30rpx;
}

.list-wrap-grid {
    flex-wrap: wrap;
    justify-content: space-between;
}

.list-wrap-column {
    flex-direction: column;
}

.list-item-grid {
    display: flex;
    flex-direction: column;
    margin-bottom: 30rpx;
    width: 330rpx;
    height: 484rpx;
    border-radius: 20rpx;
    background: #ffffff;
}

.points-goods-img {
    border-radius: 20rpx 20rpx 0px 0px;
}

.points-goods-name {
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    margin-top: 20rpx;
    margin-right: 12rpx;
    margin-left: 12rpx;
    color: var(--primary-color-text);
    text-overflow: ellipsis;
    word-break: break-all;
    font-weight: 400;
    font-size: 30rpx;
    line-height: 42rpx;

    -webkit-line-clamp: 1;
}

.points-goods-price-wrap {
    display: flex;
    justify-content: space-between;
    margin-top: 16rpx;
}

.points-goods-price {
    position: relative;
    margin-top: 8rpx;
    margin-left: 12rpx;
    color: #b2b2b2;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.points-goods-price:before {
    position: absolute;
    top: 20rpx;
    right: 0;
    left: 0;
    margin: 0 auto;
    width: 100%;
    height: 2rpx;
    background: #b2b2b2;
    content: '';
}

.points-goods-points-wrap {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-right: 12rpx;
    margin-bottom: 10rpx;
    padding-right: 20rpx;
    padding-left: 10rpx;
    border-radius: 28rpx;
    background: rgba(254, 206, 10, 0.2);
}

.ic-points {
    margin-top: 8rpx;
    margin-bottom: 8rpx;
    width: 36rpx;
    height: 36rpx;
}

.points-goods-points {
    margin-left: 10rpx;
    color: #fab001;
    font-weight: 500;
    font-size: 28rpx;
    line-height: 40rpx;
}

.coupon-item {
    position: relative;
    padding-bottom: 30rpx;
    width: 690rpx;
    height: 200rpx;
}
.coupon-item-bg {
    position: absolute;
    top: 0;
    left: 0;
    width: 690rpx;
    height: 200rpx;
}

.coupon-label {
    display: flex;
    align-items: center;
    flex-direction: column;
    width: 220rpx;
    height: 200rpx;
}

.coupon-label-price-wrap {
    display: flex;
    justify-content: center;
    margin-top: 32rpx;
}

.coupon-label-price-unit {
    margin-top: 36rpx;
    height: 48rpx;
    color: #ffffff;
    font-weight: bold;
    font-size: 40rpx;
    line-height: 48rpx;
}
.coupon-label-price {
    color: #ffffff;
    font-weight: bold;
    font-size: 80rpx;
    line-height: 94rpx;
}

.coupon-label-limit {
    color: #ffffff;
    font-weight: 500;
    font-size: 24rpx;
    line-height: 34rpx;
}

.coupon-content {
    display: flex;
    flex-direction: column;
    justify-content: center;
    box-sizing: border-box;
    padding-left: 30rpx;
    width: 300rpx;
    height: 200rpx;
}

.coupon-content-tip {
    color: var(--primary-color-text);
    font-weight: 500;
    font-size: 32rpx;
    line-height: 44rpx;
}

.coupon-content-second-tip {
    color: var(--primary-color-text);
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
}

.coupon-points {
    display: flex;
    align-items: center;
    justify-content: center;
    padding-right: 20rpx;
    padding-left: 10rpx;
    height: 56rpx;
    border-radius: 28rpx;
    background: rgba(254, 206, 10, 0.2);
}

.order-list-item {
    margin-bottom: 30rpx;
    padding: 0 30rpx;
    border-radius: 20rpx;
    background: #ffffff;
}

.order-list-user {
    padding: 20rpx 0;
    height: 40rpx;
    color: var(--primary-color-text);
    font-weight: 500;
    font-size: 28rpx;
    line-height: 40rpx;
}

.order-list-div {
    height: 1rpx;
    background: var(--divider-background-color);
}

.order-detail-wrap {
    margin-top: 40rpx;
}

.order-detail {
    display: flex;
    align-items: center;
    margin-bottom: 40rpx;
    height: 128rpx;
}

.order-detail-content {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    margin-left: 20rpx;
    width: 100%;
    height: 128rpx;
}

.order-detail-goods-name {
    display: -webkit-box;
    overflow: hidden;
    -webkit-box-orient: vertical;
    color: var(--primary-color-text);
    text-overflow: ellipsis;
    word-break: break-all;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;

    -webkit-line-clamp: 2;
}

.order-detail-info {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.order-detail-points {
    display: flex;
    color: #b2b2b2;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}

.order-detail-goods-num {
    box-sizing: border-box;
    margin-top: auto;
    padding: 8rpx 0;
    min-width: 48rpx;
    min-height: 48rpx;
    border-radius: 24rpx;
    background: var(--primary-color);
    color: #ffffff;
    text-align: center;
    font-weight: 400;
    font-size: 22rpx;
    line-height: 32rpx;
}

.bottom-loading {
    width: 100%;
}

.van-loading {
    padding: 24rpx 10rpx;
    width: 100%;
}

.no-more {
    box-sizing: border-box;
    padding: 24rpx 10rpx;
    width: 100%;
    color: var(--secondary-color-text);
    text-align: center;
    font-size: 22rpx;
}

.tabs-wrap {
    position: relative;
    display: flex;
    width: 100%;
    height: 100rpx;
    background: #fff;
    line-height: 100rpx;
}
.tabs-item {
    position: absolute;
    top: 30rpx;
    display: inline-block;
    height: 40rpx;
    color: var(--primary-color-text) !important;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
}
.tabs-goods {
    left: 160rpx;
}
.tabs-orders {
    left: 506rpx;
}
.order-num {
    position: absolute;
    top: 14rpx;
    left: 618rpx;
    box-sizing: border-box;
    padding: 0 10rpx;
    min-width: 32rpx;
    height: 32rpx;
    border-radius: 32rpx;
    background: #fb6060;
    color: #fff;
    text-align: center;
    font-weight: 600;
    font-size: 20rpx;
    line-height: 32rpx;
}
.tabs-active {
    font-weight: 600;
}
.line {
    position: absolute;
    bottom: 14rpx;
    left: 162rpx;
    width: 52rpx !important;
    height: 8rpx !important;
    border-radius: 0 !important;
    background: var(--primary-gradual-color);
    transition: all 0.3s ease-in-out;
}
.order-active {
    left: 536rpx;
}
.click-wrap {
    position: absolute;
    top: 0;
    left: 0;
    z-index: 11;
    display: flex;
    width: 100%;
    height: 100%;
}
.click-item {
    width: 50%;
    height: 100%;
}
